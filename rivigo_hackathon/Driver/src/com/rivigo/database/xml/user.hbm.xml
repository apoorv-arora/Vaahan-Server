<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.rivigo.database.classes.User" table="user">

		<id column="id" name="userId" type="java.lang.Integer">
			<generator class="native" />
		</id>
		
		<property column="name" name="name" type="java.lang.String" />
		<property column="number" name="number" type="java.lang.String" />
		<property column="created" name="created" type="java.lang.Long" />
		<property column="imageUrl" name="imageUrl" type="java.lang.String" />
		<property column="ownsTruck" name="ownsTruck" type="java.lang.Boolean" />
		<property column="panUrl" name="panUrl" type="java.lang.String" />
		<property column="rcUrl" name="rcUrl" type="java.lang.String" />
		<property column="dlUrl" name="dlUrl" type="java.lang.String" />
		<property column="insuranceUrl" name="insuranceUrl" type="java.lang.String" />
	</class>
</hibernate-mapping>